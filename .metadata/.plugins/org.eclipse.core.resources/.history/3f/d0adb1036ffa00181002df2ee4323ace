package app;

public enum Car {
	SMALL(45.0), 
	MEDIUM(60.0), 
	LARGE(75.0);
	
	private double dailyPrice;
	
	Car(double dailyPrice) {
		this.dailyPrice = dailyPrice;
	}
	
	public double getDailyPrice() {
		return this.dailyPrice;
	}
}

